<h4 class="text-center">Certificates of Origin</h4>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Shipper</th>
      <th>Consignee</th>
      <th>Farm</th>
      <th>Country</th>
      <th>Mode</th>
      <th>Order</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
  <% @cos.each do |co| %>
    <tr>
      <td><%= link_to co.id, certificate_path(co.id) %></td>
      <td><%= co.shipper %></td>
      <td><%= co.consignee %></td>
      <td><%= co.farm %></td>
      <td><%= co.country %></td>
      <td><%= co.mode%></td>
      <td><%= link_to co.order.id, order_path(co.order.id) %></td>
      <td><%= link_to "Edit", edit_certificate_path(co.id) %> | <%= link_to "Delete", certificate_path(co.id),method: :delete, data: { confirm: "Are you sure?" } %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= link_to "New Certificate of Origin", new_certificate_path %>
