<%= simple_form_for([@order, @bill_of_lading], :html => {'data-abide' => ' ' }, validate: true ) do |f| %>
<div class="row">
  <div class="large-4 columns">
    <%= f.input :document_number %>
  </div>
  <div class="large-4 columns">
    <%= f.input :exporter %>
  </div>
  <div class="large-4 columns">
    <%= f.input :export_references %>
  </div>
</div>
<div class="row">
  <div class="large-6 columns">
    <%= f.input :consignee %>
  </div>
  <div class="large-6 columns">
    <%= f.input :forwarding_agent_references %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :place_of_origin %>
  </div>
  <div class="large-3 columns">
    <%= f.input :place_of_reciept %>
  </div>
  <div class="large-3 columns">
    <%= f.input :place_of_delivery %>
  </div>
  <div class="large-3 columns">
    <%= f.input :date_of_reciept %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :precarriage %>
  </div>
  <div class="large-3 columns">
    <%= f.input :ocean_vessel %>
  </div>
  <div class="large-3 columns">
    <%= f.input :loading_port %>
  </div>
  <div class="large-3 columns">
    <%= f.input :discharge_port %>
  </div>
</div>
<div class="row">
  <div class="large-12 columns">
    <%= f.input :instructions %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :container_number %>
  </div>
  <div class="large-3 columns">
    <%= f.input :units %>
  </div>
  <div class="large-3 columns">
    <%= f.input :unit_type %>
  </div>
  <div class="large-3 columns">
    <%= f.input :description %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :volume %>
  </div>
  <div class="large-3 columns">
    <%= f.input :volume_units %>
  </div>
  <div class="large-3 columns">
    <%= f.input :gross_weight %>
  </div>
  <div class="large-3 columns">
    <%= f.input :weight_units %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :freight_charges %>
  </div>
  <div class="large-3 columns">
    <%= f.input :revenue_tons %>
  </div>
  <div class="large-3 columns">
    <%= f.input :rate %>
  </div>
  <div class="large-3 columns">
    <%= f.input :prepaid %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :collect %>
  </div>
  <div class="large-3 columns">
    <%= f.input :bl_number %>
  </div>
  <div class="large-3 columns">
    <%= f.input :original_number %>
  </div>
  <div class="large-3 columns">
    <%= f.input :prepaid_at %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :collect_at %>
  </div>
  <div class="large-3 columns">
    <%= f.input :place_of_issue %>
  </div>
  <div class="large-3 columns">
    <%= f.input :issue_date %>
  </div>
  <div class="large-3 columns">
    <%= f.input :service_type %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :exchange_rate_1 %>
  </div>
  <div class="large-3 columns">
    <%= f.input :exchange_rate_2 %>
  </div>
  <div class="large-3 columns">
    <%= f.input :service_type %>
  </div>
  <div class="large-3 columns">
    <%= f.input :laden_on_board %>
  </div>
</div>
<div class="row">
  <div class="large-3 columns">
    <%= f.input :shipper_id %>
  </div>
  <div class="large-3 columns">
    <%= f.input :notification %>
  </div>
  <div class="large-3 columns">
    <%= f.input :also_notify %>
  </div>
  <div class="large-3 columns">
    <%= f.input :order %>
  </div>
</div>
<div class="actions row">
  <%= f.button :submit, class: 'expanded' %>
</div>
<% end %>
